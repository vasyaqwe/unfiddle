{
   "name": "unfiddle-api",
   "main": "src/index.ts",
   "compatibility_flags": ["nodejs_compat"],
   "compatibility_date": "2025-04-01",
   "observability": {
      "enabled": true,
      "head_sampling_rate": 1
   },
   "placement": {
      "mode": "smart"
   },
   "ai": {
      "binding": "AI"
   },
   "unsafe": {
      "bindings": [
         {
            "name": "RATE_LIMITER",
            "type": "ratelimit",
            "namespace_id": "1001",
            "simple": {
               "limit": 10,
               "period": 60
            }
         }
      ]
   },
   "vars": {
      "ENVIRONMENT": "development"
   },
   "env": {
      "production": {
         "name": "unfiddle-api",
         "vars": {
            "ENVIRONMENT": "production"
         }
      }
   }
}
