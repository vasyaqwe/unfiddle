{
   "name": "unfiddle-api",
   "main": "src/index.ts",
   "compatibility_flags": ["nodejs_compat"],
   "compatibility_date": "2025-04-01",
   "observability": {
      "enabled": true,
      "head_sampling_rate": 1
   },
   "placement": {
      "mode": "smart"
   },
   "ai": {
      "binding": "AI"
   },
   "unsafe": {
      "bindings": [
         {
            "name": "RATE_LIMITER",
            "type": "ratelimit",
            "namespace_id": "1001",
            "simple": {
               "limit": 10,
               "period": 60
            }
         }
      ]
   },
   "r2_buckets": [
      {
         "binding": "BUCKET",
         "bucket_name": "unfiddle"
      }
   ],
   "kv_namespaces": [
      {
         "binding": "KV",
         "id": "3b6ee6791b304ec395679ae8e8228751"
      }
   ],
   "d1_databases": [
      {
         "binding": "DATABASE",
         "database_name": "unfiddle",
         "database_id": "b3c5c658-0eec-4ad5-95b2-63948fd304dd",
         "migrations_dir": "../../packages/core/src/database/migrations"
      }
   ],
   "vars": {
      "ENVIRONMENT": "development"
   },
   "env": {
      "production": {
         "name": "unfiddle-api",
         "ai": {
            "binding": "AI"
         },
         "unsafe": {
            "bindings": [
               {
                  "name": "RATE_LIMITER",
                  "type": "ratelimit",
                  "namespace_id": "1001",
                  "simple": {
                     "limit": 10,
                     "period": 60
                  }
               }
            ]
         },
         "r2_buckets": [
            {
               "binding": "BUCKET",
               "bucket_name": "unfiddle"
            }
         ],
         "kv_namespaces": [
            {
               "binding": "KV",
               "id": "3b6ee6791b304ec395679ae8e8228751"
            }
         ],
         "d1_databases": [
            {
               "binding": "DATABASE",
               "database_name": "unfiddle",
               "database_id": "b3c5c658-0eec-4ad5-95b2-63948fd304dd",
               "migrations_dir": "../../packages/core/src/database/migrations"
            }
         ],
         "durable_objects": {
            "bindings": [
               {
                  "name": "Order",
                  "class_name": "Order"
               }
            ]
         },
         "migrations": [
            {
               "tag": "v1",
               "new_sqlite_classes": ["Order"]
            }
         ],
         "vars": {
            "ENVIRONMENT": "production"
         }
      }
   }
}
